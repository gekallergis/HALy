#JSGF V1.0;

/**
 * JSGF Grammar for Hello World example
 */

grammar haly;

public <basicCmd> = (Hal) <command> <subject>;

<command> = (open | close | on | off | help | locate | register | shutdown);
<subject> = (window | door | lights | oven | boiler | tent | music | fridge | ventilator | user);
